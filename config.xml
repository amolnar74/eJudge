<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns		= "http://www.w3.org/ns/widgets"
	xmlns:gap	= "http://phonegap.com/ns/1.0"
	id		= "com.amolnar.eJudge"
	version 	= "1.0.0">

	<name>eJudge</name>

	<description>
		eJudge for RG and AGG
	</description>

	<author href="http://rgform.eu"
		email="info.rgform@gmail.com">
		Attila Molnar
	</author>

	<gap:platforms>
		<gap:platform name="android" minVersion="2.1" />
		<gap:platform name="webos" />
		<gap:platform name="symbian.wrt" />
		<gap:platform name="blackberry" project="widgets"/>
	</gap:platforms>

	<icon src="blurry.png" gap:role="default" />

    <gap:splash src="weirdo.png" />
	<feature name="http://api.phonegap.com/1.0/network"/>
<access origin="*" />
<gap:plugin name="cordova-plugin-inappbrowser" source="npm" />
<!-- sample preference specification -->
	<!-- <preference name="autorotate" value="false" readonly="true"/> -->
</widget>
